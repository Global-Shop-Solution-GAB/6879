	Gui.FrmPL.GsGridControlPLStat.SetColumnProperty("gvPL","PERCENTOFSALES","Caption","% of Sales")

	Gui.FrmPL.GsGridControlPLStat.SetColumnProperty("gvPL","PRODUCT_LINE","AllowEdit",False)
